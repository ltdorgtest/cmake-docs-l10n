#: ../../../prop_test/FAIL_REGULAR_EXPRESSION.rst:2
msgid "FAIL_REGULAR_EXPRESSION"
msgstr ""

#: ../../../prop_test/FAIL_REGULAR_EXPRESSION.rst:4
msgid ""
"If the output matches this regular expression the test will fail, regardless "
"of the process exit code."
msgstr ""

#: ../../../prop_test/FAIL_REGULAR_EXPRESSION.rst:7
msgid ""
"If set, if the output matches one of specified regular expressions, the test "
"will fail.  Example:"
msgstr ""

#: ../../../prop_test/FAIL_REGULAR_EXPRESSION.rst:10
msgid ""
"set_tests_properties(mytest PROPERTIES\n"
"  FAIL_REGULAR_EXPRESSION \"[^a-z]Error;ERROR;Failed\"\n"
")"
msgstr ""

#: ../../../prop_test/FAIL_REGULAR_EXPRESSION.rst:16
msgid "``FAIL_REGULAR_EXPRESSION`` expects a list of regular expressions."
msgstr ""

