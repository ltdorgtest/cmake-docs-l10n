#: ../../../manual/cmake-presets.7.rst:890
msgid "``until-pass``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:892
msgid "``after-timeout``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:894
