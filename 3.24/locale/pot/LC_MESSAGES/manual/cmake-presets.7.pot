msgstr ""

#: ../../../manual/cmake-presets.7.rst:859
msgid "``resourceSpecFile``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:861
msgid ""
"An optional string. Equivalent to passing ``--resource-spec-file`` on the "
"command line. This field supports macro expansion."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:864
msgid "``testLoad``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:866
msgid ""
"An optional integer. Equivalent to passing ``--test-load`` on the command "
"line."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:869
msgid "``showOnly``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:871
msgid ""
"An optional string. Equivalent to passing ``--show-only`` on the command "
"line. The string must be one of the following values:"
