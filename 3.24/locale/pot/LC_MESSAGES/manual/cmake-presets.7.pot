msgstr ""

#: ../../../manual/cmake-presets.7.rst:876
msgid "``json-v1``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:878
msgid "``repeat``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:880
