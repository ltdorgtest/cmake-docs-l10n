msgid "Tools"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:176
msgid "The version of used QtIFW_ tools."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:180
msgid "The path to \"binarycreator\" command line client."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:182
#: ../../../../Modules/CPackIFW.cmake:188
