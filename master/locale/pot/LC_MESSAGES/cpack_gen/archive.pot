msgstr ""

#: ../../../cpack_gen/archive.rst:13
msgid "7Z_BZ2 - 7zip - (``.7z``) - BZip2 compressed"
msgstr ""

#: ../../../cpack_gen/archive.rst:17
msgid "7Z_DEFLATE - 7zip - (``.7z``) - Deflate compressed"
msgstr ""

#: ../../../cpack_gen/archive.rst:21
