msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:45
msgid "The path to the ``fluid2`` binary tool."
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:47
msgid "``FLTK2_INCLUDE_DIR``"
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:48
msgid "The directory containing header files needed to use FLTK 2.x."
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:50
msgid "``FLTK2_BASE_LIBRARY``"
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:51
msgid "The path to the FLTK 2.x library (``fltk2``)."
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:53
msgid "``FLTK2_GL_LIBRARY``"
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:54
msgid "The path to the FLTK 2.x OpenGL compatibility library (``fltk2_gl``)."
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:56
msgid "``FLTK2_IMAGES_LIBRARY``"
msgstr ""

#: ../../../../Modules/FindFLTK2.cmake:57
msgid "The path to the FLTK 2.x Images library (``fltk2_images``)."
msgstr ""

