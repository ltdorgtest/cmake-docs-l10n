"-- num_0=two, num_1=dua\n"
"-- num_0=three, num_1=tiga\n"
"-- num_0=four, num_1=\n"
"-- en=one, ba=satu\n"
"-- en=two, ba=dua\n"
"-- en=three, ba=tiga\n"
"-- en=four, ba="
msgstr ""

