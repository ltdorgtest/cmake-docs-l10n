msgid "Boolean indicating whether the Tcl is found."
msgstr ""

#: ../../../../Modules/FindTCL.cmake:22
msgid "``TK_FOUND``"
msgstr ""

#: ../../../../Modules/FindTCL.cmake:23
msgid "Boolean indicating whether the Tk is found."
msgstr ""

#: ../../../../Modules/FindTCL.cmake:24
msgid "``TCLTK_FOUND``"
msgstr ""

#: ../../../../Modules/FindTCL.cmake:25
msgid "Boolean indicating whether both Tcl and Tk are found."
msgstr ""

