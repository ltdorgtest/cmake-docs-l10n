#: ../../../manual/ctest.1.rst:964 ../../../manual/ctest.1.rst:1174
#: ../../../manual/ctest.1.rst:1203 ../../../manual/ctest.1.rst:1260
#: ../../../manual/ctest.1.rst:1307 ../../../manual/ctest.1.rst:1333
#: ../../../manual/ctest.1.rst:1435
msgid "Configuration settings include:"
msgstr ""

#: ../../../manual/ctest.1.rst:966
msgid "``BuildDirectory``"
msgstr ""

#: ../../../manual/ctest.1.rst:967
