msgid "The full version string as displayed by cmake ``--version``."
msgstr ""

#: ../../../manual/cmake.1.rst:540
msgid "``major``"
msgstr ""

#: ../../../manual/cmake.1.rst:541
msgid "The major version number in integer form."
