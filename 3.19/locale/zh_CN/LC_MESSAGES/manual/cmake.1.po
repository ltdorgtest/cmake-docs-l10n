#: ../../../manual/cmake.1.rst:542
msgid "``minor``"
msgstr ""

#: ../../../manual/cmake.1.rst:543
msgid "The minor version number in integer form."
msgstr ""

#: ../../../manual/cmake.1.rst:544
msgid "``patch``"
msgstr ""

#: ../../../manual/cmake.1.rst:545
msgid "The patch level in integer form."
msgstr ""

#: ../../../manual/cmake.1.rst:546
msgid "``suffix``"
msgstr ""

#: ../../../manual/cmake.1.rst:547
msgid "The cmake version suffix string."
msgstr ""

#: ../../../manual/cmake.1.rst:548
msgid "``isDirty``"
msgstr ""

#: ../../../manual/cmake.1.rst:549
msgid "A bool that is set if the cmake build is from a dirty tree."
msgstr ""

