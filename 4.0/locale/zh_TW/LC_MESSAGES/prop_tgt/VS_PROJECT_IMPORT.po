#: ../../../prop_tgt/VS_PROJECT_IMPORT.rst:2
msgid "VS_PROJECT_IMPORT"
msgstr ""

#: ../../../prop_tgt/VS_PROJECT_IMPORT.rst:6
msgid "Visual Studio managed project imports"
msgstr ""

#: ../../../prop_tgt/VS_PROJECT_IMPORT.rst:8
msgid ""
"Adds to a generated Visual Studio project one or more paths to ``.props`` "
"files needed when building projects from some NuGet packages."
msgstr ""

#: ../../../prop_tgt/VS_PROJECT_IMPORT.rst:11
msgid "For example:"
msgstr ""

#: ../../../prop_tgt/VS_PROJECT_IMPORT.rst:13
msgid ""
"set_property(TARGET myTarget PROPERTY VS_PROJECT_IMPORT\n"
"  \"my_packages_path/PackageA.1.0.0/build/PackageA.props\"\n"
"  \"my_packages_path/PackageB.1.0.0/build/PackageB.props\"\n"
")"
msgstr ""
