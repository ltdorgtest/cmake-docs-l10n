msgid "Examples"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:28
msgid "Example: Linking Dynamic Loading Library"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:30
msgid ""
"Using this variable in a project that uses dynamic loading functionality:"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:32
msgid "target_link_libraries(example PRIVATE ${CMAKE_DL_LIBS})"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:37
msgid "Example: Checking Symbols"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:39
msgid ""
"Checking for symbols with the dynamic loading library linked during the "
"check:"
msgstr ""

#: ../../../variable/CMAKE_DL_LIBS.rst:41
msgid ""
"include(CheckSymbolExists)\n"
"include(CMakePushCheckState)\n"
"\n"
"cmake_push_check_state(RESET)\n"
"  set(CMAKE_REQUIRED_LIBRARIES ${CMAKE_DL_LIBS})\n"
"  check_symbol_exists(dlopen \"dlfcn.h\" HAVE_DLOPEN)\n"
"cmake_pop_check_state()"
msgstr ""
