msgstr ""

#: ../../../manual/cmake-instrumentation.7.rst:504
msgid "``numberOfLogicalCPU``"
msgstr ""

#: ../../../manual/cmake-instrumentation.7.rst:505
msgid "``numberOfPhysicalCPU``"
msgstr ""

#: ../../../manual/cmake-instrumentation.7.rst:506
msgid "``processorAPICID``"
msgstr ""

#: ../../../manual/cmake-instrumentation.7.rst:507
msgid "``processorCacheSize``"
msgstr ""

#: ../../../manual/cmake-instrumentation.7.rst:508
