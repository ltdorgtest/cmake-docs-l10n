#: ../../../manual/cmake-presets.7.rst:789
msgid "``until-fail``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:791
msgid "``until-pass``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:793
msgid "``after-timeout``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:795
msgid "``count``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:797
msgid "A required integer."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:799
msgid "``interactiveDebugging``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:801
