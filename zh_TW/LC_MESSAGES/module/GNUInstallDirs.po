msgid "where <dir> is one of:"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:20
msgid "``BINDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:21
msgid "user executables (bin)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:22
msgid "``SBINDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:23
msgid "system admin executables (sbin)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:24
msgid "``LIBEXECDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:25
msgid "program executables (libexec)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:26
msgid "``SYSCONFDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:27
msgid "read-only single-machine data (etc)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:28
msgid "``SHAREDSTATEDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:29
msgid "modifiable architecture-independent data (com)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:30
msgid "``LOCALSTATEDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:31
msgid "modifiable single-machine data (var)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:32
msgid "``LIBDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:33
msgid "object code libraries (lib or lib64 or lib/<multiarch-tuple> on Debian)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:34
msgid "``INCLUDEDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:35
msgid "C header files (include)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:36
msgid "``OLDINCLUDEDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:37
msgid "C header files for non-gcc (/usr/include)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:38
msgid "``DATAROOTDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:39
msgid "read-only architecture-independent data root (share)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:40
msgid "``DATADIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:41
msgid "read-only architecture-independent data (DATAROOTDIR)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:42
msgid "``INFODIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:43
msgid "info documentation (DATAROOTDIR/info)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:44
msgid "``LOCALEDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:45
msgid "locale-dependent data (DATAROOTDIR/locale)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:46
msgid "``MANDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:47
msgid "man documentation (DATAROOTDIR/man)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:49
msgid "``DOCDIR``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:49
msgid "documentation root (DATAROOTDIR/doc/PROJECT_NAME)"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:51
msgid ""
"Each CMAKE_INSTALL_<dir> value may be passed to the DESTINATION options of "
"install() commands for the corresponding file type.  If the includer does "
"not define a value the above-shown default will be used and the value will "
"appear in the cache for editing by the user.  Each CMAKE_INSTALL_FULL_<dir> "
"value contains an absolute path constructed from the corresponding "
"destination by prepending (if necessary) the value of CMAKE_INSTALL_PREFIX."
msgstr ""
