msgid "GNUInstallDirs"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:5
msgid "Define GNU standard installation directories"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:7
msgid "Provides install directory variables as defined for GNU software:"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:9
msgid "http://www.gnu.org/prep/standards/html_node/Directory-Variables.html"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:11
msgid "Inclusion of this module defines the following variables:"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:13
msgid "``CMAKE_INSTALL_<dir>``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:14
msgid "destination for files of a given type"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:16
msgid "``CMAKE_INSTALL_FULL_<dir>``"
msgstr ""

#: ../../../../Modules/GNUInstallDirs.cmake:16
msgid "corresponding absolute path"
msgstr ""

