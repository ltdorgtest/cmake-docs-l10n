msgid "Mandatory : YES"
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:36
msgid "Default   : CPACK_PACKAGE_DESCRIPTION_SUMMARY"
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:40
msgid "The RPM package name."
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:43
msgid "Default   : CPACK_PACKAGE_NAME"
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:47
msgid "The RPM package version."
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:50
msgid "Default   : CPACK_PACKAGE_VERSION"
msgstr ""

#: ../../../../Modules/CPackRPM.cmake:54
msgid "The RPM package architecture."
msgstr ""

