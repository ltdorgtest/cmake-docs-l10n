#: ../../../../Modules/FindXercesC.cmake:3
msgid "FindXercesC"
msgstr ""

#: ../../../../Modules/FindXercesC.cmake:5
msgid "Find the Apache Xerces-C++ validating XML parser headers and libraries."
msgstr ""

#: ../../../../Modules/FindXercesC.cmake:7
msgid ""
"This module reports information about the Xerces installation in several "
"variables.  General variables::"
msgstr ""

#: ../../../../Modules/FindXercesC.cmake:10
msgid ""
"XercesC_FOUND - true if the Xerces headers and libraries were found\n"
"XercesC_VERSION - Xerces release version\n"
"XercesC_INCLUDE_DIRS - the directory containing the Xerces headers\n"
"XercesC_LIBRARIES - Xerces libraries to be linked"
msgstr ""

#: ../../../../Modules/FindXercesC.cmake:15
msgid "The following cache variables may also be set::"
msgstr ""

#: ../../../../Modules/FindXercesC.cmake:17
msgid ""
"XercesC_INCLUDE_DIR - the directory containing the Xerces headers\n"
"XercesC_LIBRARY - the Xerces library"
msgstr ""
