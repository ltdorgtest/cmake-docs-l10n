#: ../../../manual/cmake-presets.7.rst:835
msgid ""
"An optional integer. Equivalent to passing ``--parallel`` on the command "
"line."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:838
msgid "``resourceSpecFile``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:840
msgid ""
"An optional string. Equivalent to passing ``--resource-spec-file`` on the "
"command line. This field supports macro expansion."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:843
msgid "``testLoad``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:845
msgid ""
"An optional integer. Equivalent to passing ``--test-load`` on the command "
"line."
msgstr ""

#: ../../../manual/cmake-presets.7.rst:848
msgid "``showOnly``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:850
msgid ""
"An optional string. Equivalent to passing ``--show-only`` on the command "
"line. The string must be one of the following values:"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:853
msgid "``human``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:855
msgid "``json-v1``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:857
msgid "``repeat``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:859
msgid ""
"An optional object specifying how to repeat tests. Equivalent to passing ``--"
