msgstr ""

#: ../../../manual/cmake-presets.7.rst:869
msgid "``until-pass``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:871
msgid "``after-timeout``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:873
msgid "``count``"
msgstr ""

#: ../../../manual/cmake-presets.7.rst:875
msgid "A required integer."
