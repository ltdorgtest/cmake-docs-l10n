#: ../../../../Modules/FindProtobuf.cmake:3
msgid "FindProtobuf"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:7
msgid "Locate and configure the Google Protocol Buffers library."
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:9
msgid "The following variables can be set and are optional:"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:13
msgid ""
"PROTOBUF_SRC_ROOT_FOLDER - When compiling with MSVC, if this cache variable "
"is set\n"
"                           the protobuf-default VS project build locations\n"
"                           (vsprojects/Debug & vsprojects/Release) will be "
"searched\n"
"                           for libraries and binaries."
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:22
msgid ""
"PROTOBUF_IMPORT_DIRS     - List of additional directories to be searched "
"for\n"
"                           imported .proto files. (New in CMake 2.8.8)"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:27
msgid "Defines the following variables:"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:31
msgid ""
"PROTOBUF_FOUND - Found the Google Protocol Buffers library (libprotobuf & "
"header files)\n"
"PROTOBUF_INCLUDE_DIRS - Include directories for Google Protocol Buffers\n"
"PROTOBUF_LIBRARIES - The protobuf libraries"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:35
#: ../../../../Modules/FindProtobuf.cmake:53
msgid "[New in CMake 2.8.5]"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:39
msgid ""
"PROTOBUF_PROTOC_LIBRARIES - The protoc libraries\n"
"PROTOBUF_LITE_LIBRARIES - The protobuf-lite libraries"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:44
msgid "The following cache variables are also available to set or use:"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:48
msgid ""
"PROTOBUF_LIBRARY - The protobuf library\n"
"PROTOBUF_PROTOC_LIBRARY   - The protoc library\n"
"PROTOBUF_INCLUDE_DIR - The include directory for protocol buffers\n"
"PROTOBUF_PROTOC_EXECUTABLE - The protoc compiler"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:57
msgid ""
"PROTOBUF_LIBRARY_DEBUG - The protobuf library (debug)\n"
"PROTOBUF_PROTOC_LIBRARY_DEBUG   - The protoc library (debug)\n"
"PROTOBUF_LITE_LIBRARY - The protobuf lite library\n"
"PROTOBUF_LITE_LIBRARY_DEBUG - The protobuf lite library (debug)"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:66
msgid ""
"====================================================================\n"
"Example:"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:73
msgid ""
"find_package(Protobuf REQUIRED)\n"
"include_directories(${PROTOBUF_INCLUDE_DIRS})"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:80
msgid ""
"include_directories(${CMAKE_CURRENT_BINARY_DIR})\n"
"PROTOBUF_GENERATE_CPP(PROTO_SRCS PROTO_HDRS foo.proto)\n"
"add_executable(bar bar.cc ${PROTO_SRCS} ${PROTO_HDRS})\n"
"target_link_libraries(bar ${PROTOBUF_LIBRARIES})"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:87
msgid "NOTE: You may need to link against pthreads, depending"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:91
msgid "on the platform."
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:95
msgid ""
"NOTE: The PROTOBUF_GENERATE_CPP macro & add_executable() or add_library()"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:100
msgid "calls only work properly within the same directory."
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:106
#: ../../../../Modules/FindProtobuf.cmake:124
msgid "===================================================================="
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:110
msgid "PROTOBUF_GENERATE_CPP (public function)"
msgstr ""

#: ../../../../Modules/FindProtobuf.cmake:114
msgid ""
"SRCS = Variable to define with autogenerated\n"
"       source files\n"
"HDRS = Variable to define with autogenerated\n"
"       header files\n"
"ARGN = proto files"
msgstr ""
