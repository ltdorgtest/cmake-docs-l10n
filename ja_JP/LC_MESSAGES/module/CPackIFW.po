msgstr ""

#: ../../../../Modules/CPackIFW.cmake:180
msgid "Commands"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:182
msgid "The module defines the following commands:"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:188
#: ../../../../Modules/CPackIFW.cmake:228
msgid "Sets the arguments specific to the CPack IFW generator."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:192
msgid ""
"cpack_ifw_configure_component(<compname> [COMMON] [ESSENTIAL]\n"
"                    [NAME <name>]\n"
"                    [VERSION <version>]\n"
"                    [SCRIPT <script>]\n"
"                    [PRIORITY <priority>]\n"
"                    [DEPENDS <com_id> ...]\n"
"                    [LICENSES <display_name> <file_path> ...])"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:200
msgid ""
"This command should be called after :command:`cpack_add_component` command."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:202
msgid ""
"``COMMON`` if set, then the component will be packaged and installed as part "
"of a group to which it belongs."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:205
msgid ""
"``ESSENTIAL`` if set, then the package manager stays disabled until that "
"component is updated."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:208
msgid ""
"``NAME`` is used to create domain-like identification for this component. By "
"default used origin component name."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:211
msgid ""
"``VERSION`` is version of component. By default used :variable:"
"`CPACK_PACKAGE_VERSION`."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:214
msgid ""
"``SCRIPT`` is a relative or absolute path to operations script for this "
"component."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:217
msgid "``PRIORITY`` is priority of the component in the tree."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:219
msgid "``DEPENDS`` list of dependency component identifiers in QtIFW_ style."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:221
msgid ""
"``LICENSES`` pair of <display_name> and <file_path> of license text for this "
"component. You can specify more then one license."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:232
msgid ""
"cpack_ifw_configure_component_group(<groupname>\n"
"                    [NAME <name>]\n"
"                    [VERSION <version>]\n"
"                    [SCRIPT <script>]\n"
"                    [PRIORITY <priority>]\n"
"                    [LICENSES <display_name> <file_path> ...])"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:239
msgid ""
"This command should be called after :command:`cpack_add_component_group` "
"command."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:242
msgid ""
"``NAME`` is used to create domain-like identification for this component "
"group. By default used origin component group name."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:246
msgid ""
"``VERSION`` is version of component group. By default used :variable:"
"`CPACK_PACKAGE_VERSION`."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:249
msgid ""
"``SCRIPT`` is a relative or absolute path to operations script for this "
"component group."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:252
msgid "``PRIORITY`` is priority of the component group in the tree."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:254
msgid ""
"``LICENSES`` pair of <display_name> and <file_path> of license text for this "
"component group. You can specify more then one license."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:261
msgid "Add QtIFW_ specific remote repository to binary installer."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:265
msgid ""
"cpack_ifw_add_repository(<reponame> [DISABLED]\n"
"                    URL <url>\n"
"                    [USERNAME <username>]\n"
"                    [PASSWORD <password>]\n"
"                    [DISPLAY_NAME <display_name>])"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:271
#: ../../../../Modules/CPackIFW.cmake:300
msgid ""
"This command will also add the <reponame> repository to a variable :variable:"
"`CPACK_IFW_REPOSITORIES_ALL`."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:274
msgid "``DISABLED`` if set, then the repository will be disabled by default."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:276
#: ../../../../Modules/CPackIFW.cmake:303
msgid "``URL`` is points to a list of available components."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:278
#: ../../../../Modules/CPackIFW.cmake:309
msgid "``USERNAME`` is used as user on a protected repository."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:280
#: ../../../../Modules/CPackIFW.cmake:311
msgid "``PASSWORD`` is password to use on a protected repository."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:282
#: ../../../../Modules/CPackIFW.cmake:313
msgid "``DISPLAY_NAME`` is string to display instead of the URL."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:289
msgid "Update QtIFW_ specific repository from remote repository."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:293
msgid ""
"cpack_ifw_update_repository(<reponame>\n"
"                    [[ADD|REMOVE] URL <url>]|\n"
"                     [REPLACE OLD_URL <old_url> NEW_URL <new_url>]]\n"
"                    [USERNAME <username>]\n"
"                    [PASSWORD <password>]\n"
"                    [DISPLAY_NAME <display_name>])"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:305
msgid "``OLD_URL`` is points to a list that will replaced."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:307
msgid "``NEW_URL`` is points to a list that will replace to."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:316
msgid "Example usage"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:318
msgid ""
"set(CPACK_PACKAGE_NAME \"MyPackage\")\n"
"set(CPACK_PACKAGE_DESCRIPTION_SUMMARY \"MyPackage Installation Example\")\n"
"set(CPACK_PACKAGE_VERSION \"1.0.0\") # Version of installer\n"
"\n"
"include(CPack)\n"
"include(CPackIFW)\n"
"\n"
"cpack_add_component(myapp\n"
"    DISPLAY_NAME \"MyApp\"\n"
"    DESCRIPTION \"My Application\")\n"
"cpack_ifw_configure_component(myapp\n"
"    VERSION \"1.2.3\" # Version of component\n"
"    SCRIPT \"operations.qs\")\n"
"cpack_add_component(mybigplugin\n"
"    DISPLAY_NAME \"MyBigPlugin\"\n"
"    DESCRIPTION \"My Big Downloadable Plugin\"\n"
"    DOWNLOADED)\n"
"cpack_ifw_add_repository(myrepo\n"
"    URL \"http://example.com/ifw/repo/myapp\"\n"
"    DISPLAY_NAME \"My Application Repository\")"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:343
msgid "Online installer"
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:345
msgid ""
"By default CPack IFW generator makes offline installer. This means that all "
"components will be packaged into a binary file."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:348
msgid ""
"To make a component downloaded, you must set the ``DOWNLOADED`` option in :"
"command:`cpack_add_component`."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:351
msgid ""
"Then you would use the command :command:`cpack_configure_downloads`. If you "
"set ``ALL`` option all components will be downloaded."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:354
msgid ""
"You also can use command :command:`cpack_ifw_add_repository` and variable :"
"variable:`CPACK_IFW_DOWNLOAD_ALL` for more specific configuration."
msgstr ""

#: ../../../../Modules/CPackIFW.cmake:357
msgid ""
"CPack IFW generator creates \"repository\" dir in current binary dir. You "
"would copy content of this dir to specified ``site`` (``url``)."
msgstr ""

